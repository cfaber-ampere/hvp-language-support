{
    "hvp_plan":{
        "prefix":"pl",
        "body":[
            "plan ${1:plan_name};",
            "\t$0",
            "endplan\n"
        ],
        "description":"add plan"
    },
    "hvp_metric":{
        "prefix":"met",
        "body":[
            "metric ${1:metric_type} ${2:metric_name};",
            "\t$0",
            "endmetric\n"
        ],
        "description":"add metric"
    },
    "hvp_attribute":{
        "prefix":"att",
        "body":[
            "attribute ${1:attribute_type} ${2:attribute_name};\n"
        ],
        "description":"add attribute"
    },
    "hvp_feature":{
        "prefix":"fea",
        "body":[
            "feature \"${1:feature_name}\";",
            "\towner = \"${2:owner_name}\";",
            "\tdescription = \"${3:description_text}\";",
            "\tType = ${4:feature_type};",
            "\tPriority = ${5:priority};",
            "\tEnvironment = \\{${6:environments}\\};",
            "\tProject = ${7:project_name};",
            "\tStatus = ${8:status};",
            "endfeature\n"
        ],
        "description":"add new feature"
    }
}