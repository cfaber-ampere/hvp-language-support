{
    "hvp_plan":{
        "prefix":"pl",
        "body":[
            "plan ${1:plan_name};",
            "\t$0",
            "endplan\n"
        ],
        "description":"add plan"
    },
    "hvp_metric":{
        "prefix":"met",
        "body":[
            "metric ${1:metric_type} ${2:metric_name};",
            "\t$0",
            "endmetric\n"
        ],
        "description":"add metric"
    },
    "hvp_attribute":{
        "prefix":"att",
        "body":[
            "attribute ${1:attribute_type} ${2:attribute_name};\n"
        ],
        "description":"add attribute"
    },
    "hvp_feature":{
        "prefix":"feature",
        "body":[
            "feature \"${1:FEATURE_NAME}\";",
            "\towner = \"${2:OWNER}\";",
            "\tdescription = \"${3:DESCRIPTION}\";",
            "\tType = ${4:TYPE};",
            "\tPriority = ${5:PRIORITY};",
            "\tEnvironment = \\{${6:ENVIRONMENT}\\};",
            "\tProject = ${7:PROJECT};",
            "\tStatus = ${8:STATUS};",
            "endfeature\n$0"
        ],
        "description":"add new feature"
    },
    "hvp_group_measure":{
        "prefix":"measure Group",
        "body":[
            "measure Group GroupMeasure;",
            "\tsource = \"group: cpu_tb.th.cpu.Mec.Lsu.lsu_cov::${1:COVERGROUP}.${2:COVERPOINT}\";",
            "endmeasure\n$0"
        ],
        "description":"add new group measure"
    },
    "hvp_assert_measure":{
        "prefix":"measure Assert",
        "body":[
            "measure Assert AssertMeasure;",
            "\tsource = \"property: cpu_tb.th.cpu.Mec.Lsu.lsu_cov.${1:PROPERTY_NAME}\";",
            "endmeasure\n$0"
        ],
        "description":"add new assert measure"
    }
}